block variables
include /partials/mixins

doctype html
html(lang="en-US")
  head
    <!-- Global site tag (gtag.js) - Google Analytics -->
    script(async src="https://www.googletagmanager.com/gtag/js?id=UA-171805476-1")
    script.
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-171805476-1');

    title= `Brad Stell - ${setName}`
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0 maximum-scale=2.0")
    meta(http-equiv="content-language" content="en")
    link(rel="apple-touch-icon" sizes="180x180" href="./apple-touch-icon.png")
    link(rel="icon" type="image/png" sizes="32x32" href="./favicon-32x32.png")
    link(rel="icon" type="image/png" sizes="16x16" href="./favicon-16x16.png")
    link(rel="manifest" href="./site.webmanifest")
    style
      include ../../../public/styles/jamesbradley.css
  body
    div.set-container-wrapper
      include ./set-header
      div.set-navigation-container-tablet
        +set_nav(setName)
      div.set-container
        div.nav-container
          +set_nav(setName)
        div.content
          block content
    script(src="js/index.js" defer)
